{
	"name": "threex.htmlmixer-continued",
	"version": "0.0.2",
	"description": "3D html mixer by @jeromeetienne",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Neosoulink/threex.htmlmixer-continued.git"
	},
	"keywords": [
		"threejs",
		"html",
		"2D",
		"3D",
		"js",
		"ts"
	],
	"main": "./lib/index.js",
	"types": "./lib/index.d.ts",
	"author": "Neosoulink,jeromeetienne",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/Neosoulink/threex.htmlmixer-continued/issues"
	},
	"homepage": "https://github.com/Neosoulink/threex.htmlmixer-continued#readme",
	"scripts": {
		"test": "jest --config ./jest.config.js",
		"build": "tsc",
		"format": "prettier --write \"src/**/*.ts\"",
		"lint": "eslint --ext .ts,.js ./src",
		"prepare": "npm run build",
		"prepublishOnly": "npm test && npm run lint",
		"preversion": "npm run lint",
		"version": "npm run format && git add -A src",
		"postversion": "git push && git push --tags",
		"release": "npm run prepare && npx np"
	},
	"dependencies": {
		"three": "^0.161.0"
	},
	"devDependencies": {
		"@types/jest": "^29.5.11",
		"@types/three": "^0.161.0",
		"@typescript-eslint/eslint-plugin": "^6.1.0",
		"@typescript-eslint/parser": "^6.1.0",
		"eslint": "^8.45.0",
		"jest": "^29.5.0",
		"jest-environment-jsdom": "^29.7.0",
		"prettier": "^3.1.0",
		"ts-jest": "^29.1.0",
		"ts-node": "^10.9.1",
		"typescript": "^5.0.4"
	},
	"np": {
		"yarn": false,
		"contents": "lib"
	},
	"files": [
		"lib/**/*"
	],
	"engines": {
		"node": ">=17.0.0"
	}
}
